FMANALYSER-MIB DEFINITIONS ::= BEGIN


IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
    	FROM SNMPv2-CONF
    enterprises, MODULE-IDENTITY, OBJECT-TYPE,
    Integer32, Unsigned32
        FROM SNMPv2-SMI;


fmanalyser MODULE-IDENTITY
    LAST-UPDATED    "%(now)s"
    ORGANIZATION    "Gaftech"
    CONTACT-INFO    "gabriel@gaftech.fr"
    DESCRIPTION     "The MIB module for fmanalyser snmp plugin"
    REVISION        "%(now)s"
    DESCRIPTION     "auto-generated revision with fmanalyser 0.1.dev"
    ::= { gaftech 1 }

gaftech OBJECT IDENTIFIER ::= { enterprises 999999 }

-- Root nodes ****************************************
device		OBJECT IDENTIFIER ::= { fmanalyser 1 }
analyser	OBJECT IDENTIFIER ::= { fmanalyser 2 }
software	OBJECT IDENTIFIER ::= { fmanalyser 8 }
conformance OBJECT IDENTIFIER ::= { fmanalyser 9 }


-- Software infos ************************************
softVersion OBJECT-TYPE
    SYNTAX        OCTET STRING
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION   "fmanalyser software version"
    ::= { software 1 }

-- Device status *************************************
online OBJECT-TYPE
    SYNTAX        Integer32(0..1)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The device online/offline status."
    ::= { device 2 }

-- Analyser root *************************************
channels			OBJECT IDENTIFIER ::= { analyser 1 }
channelVariables 	OBJECT IDENTIFIER ::= { analyser 2 }

-- Channels ******************************************
channelTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF ChannelEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "channel table"
    ::= { channels 1 }

channelEntry OBJECT-TYPE
    SYNTAX       ChannelEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "entry in channel table"
    INDEX        { channelIndex }
    ::= { channelTable 1 }

ChannelEntry ::= SEQUENCE {
    channelIndex		Unsigned32,
    channelName			OCTET STRING,
    channelRefFrequency	Unsigned32
    }

channelIndex OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "channel index"
    ::= { channelEntry 1 }

channelName OBJECT-TYPE
    SYNTAX        OCTET STRING
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "channel identifer"
    ::= { channelEntry 2 }
    
channelRefFrequency OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "configured channel frequency"
    ::= { channelEntry 3 }

-- Variables *****************************************

%(variables)s







-- Conformance information ***************************
compliances OBJECT IDENTIFIER ::= { conformance 1 }
groups		OBJECT IDENTIFIER ::= { conformance 2 }

staticValues OBJECT-GROUP
	OBJECTS {	softVersion,
				channelName
			}
	STATUS		current
	DESCRIPTION	"static informations"
	::= { groups 1 }

liveValues OBJECT-GROUP
	OBJECTS		{online}
	STATUS		current
	DESCRIPTION	"live informations"
	::= { groups 2 }

cachedValues OBJECT-GROUP
	OBJECTS		{channelRefFrequency}
	STATUS		current
	DESCRIPTION	"cached acquired data"
	::= { groups 3 }

mibCompliance MODULE-COMPLIANCE
	STATUS		current
	DESCRIPTION	"our compliance"
	MODULE
	MANDATORY-GROUPS {staticValues, liveValues, cachedValues}
	::= { compliances 1 }






END
